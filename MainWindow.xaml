                <TextBox x:Name="TextBoxBronze2" Height="23" TextWrapping="Wrap" Text="TextBox" Margin="80,0,0,0" Background="SaddleBrown" HorizontalAlignment="Left" Width="35"/>
            </Grid>
            <Label x:Name="ItemPriceCopy2" Margin="0,0,0,0" Content="Item price" Width="64" HorizontalAlignment="Left"/>
        </StackPanel>
        <StackPanel Visibility="Visible" HorizontalAlignment="Left" Height="140" Margin="455,110,0,0" VerticalAlignment="Top" Width="140" Background="DarkRed">
            <Grid>
                <Image x:Name="ItemImage3" Height="56" HorizontalAlignment="Left" Width="56" Source="img/56x56.png" />
                <Label x:Name="ItemName3" Content="Item name" HorizontalAlignment="Left" Width="162" Margin="60,0,0,0"/>
