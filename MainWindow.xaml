                <TextBox x:Name="TextBoxSilver3" Height="23" TextWrapping="Wrap" Text="TextBox" Margin="40,0,0,0" Background="Silver" HorizontalAlignment="Left" Width="35"/>
                <TextBox x:Name="TextBoxBronze3" Height="23" TextWrapping="Wrap" Text="TextBox" Margin="80,0,0,0" Background="SaddleBrown" HorizontalAlignment="Left" Width="35"/>
            </Grid>
            <Label x:Name="ItemPriceCopy3" Margin="0,0,0,0" Content="Item price" Width="64" HorizontalAlignment="Left"/>
        </StackPanel>
        <StackPanel Visibility="Visible" HorizontalAlignment="Left" Height="140" Margin="600,110,0,0" VerticalAlignment="Top" Width="140" Background="DarkRed">
            <Grid>
                <Image x:Name="ItemImage4" Height="56" HorizontalAlignment="Left" Width="56" Source="img/56x56.png" />
                <Label x:Name="ItemName4" Content="Item name" HorizontalAlignment="Left" Width="162" Margin="60,0,0,0"/>
                <Label x:Name="ItemPrice4" Content="Item price" Margin="60,24,0,0" HorizontalAlignment="Left" Width="162"/>
            </Grid>
            <TextBox x:Name="TextBoxQuantity4" Height="23" TextWrapping="Wrap" Text="TextBox" HorizontalAlignment="Left" Width="115"/>
            <Grid>
                <TextBox x:Name="TextBoxGold4" Height="23" TextWrapping="Wrap" Text="TextBox" Background="Gold" HorizontalAlignment="Left" Width="35"/>
                <TextBox x:Name="TextBoxSilver4" Height="23" TextWrapping="Wrap" Text="TextBox" Margin="40,0,0,0" Background="Silver" HorizontalAlignment="Left" Width="35"/>
                <TextBox x:Name="TextBoxBronze4" Height="23" TextWrapping="Wrap" Text="TextBox" Margin="80,0,0,0" Background="SaddleBrown" HorizontalAlignment="Left" Width="35"/>
            </Grid>
            <Label x:Name="ItemPriceCopy4" Margin="0,0,0,0" Content="Item price" Width="64" HorizontalAlignment="Left"/>
        </StackPanel>
        <StackPanel Visibility="Visible" HorizontalAlignment="Left" Height="140" Margin="20,269,0,0" VerticalAlignment="Top" Width="140" Background="#668B0000">
            <Grid>
                <Image x:Name="ItemImage5" Height="56" HorizontalAlignment="Left" Width="56" Source="img/56x56.png" />
                <Label x:Name="ItemName5" Content="Item name" HorizontalAlignment="Left" Width="162" Margin="60,0,0,0" Foreground="White"/>
                <Label x:Name="ItemPrice5" Content="Item price" Margin="60,24,0,0" HorizontalAlignment="Left" Width="162" Foreground="White"/>
            </Grid>
            <TextBox x:Name="TextBoxQuantity5" Height="23" TextWrapping="Wrap" Text="TextBox" HorizontalAlignment="Left" Width="115"/>
            <Grid>
                <TextBox x:Name="TextBoxGold5" Height="23" TextWrapping="Wrap" Text="TextBox" Foreground="Gold" HorizontalAlignment="Left" Width="35" Background="Transparent" FontWeight="Bold" FontSize="16"/>
                <TextBox x:Name="TextBoxSilver5" Height="23" TextWrapping="Wrap" Text="TextBox" Margin="40,0,0,0" Foreground="Silver" HorizontalAlignment="Left" Width="35" Background="Transparent" FontWeight="Bold" FontSize="16"/>
                <TextBox x:Name="TextBoxBronze5" Height="23" TextWrapping="Wrap" Text="TextBox" Margin="80,0,0,0" Foreground="#FFF17F2E" HorizontalAlignment="Left" Width="35" Background="Transparent" FontWeight="Bold" FontSize="16"/>
            </Grid>
            <Label x:Name="ItemPriceCopy5" Margin="0,0,0,0" Content="Item price" Width="64" HorizontalAlignment="Left" Foreground="White"/>
        </StackPanel>

    </Grid>
</Window>
